{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
      "config:recommended",
      "docker:pinDigests",
      "helpers:pinGitHubActionDigests",
      ":configMigration",
      ":pinOnlyDevDependencies"
    ],
    "automerge": true,
    "automergeType": "pr",
    "bumpVersion": "patch",
    "labels": ["dependencies"],
    "prConcurrentLimit": 1,
    "prHourlyLimit": 2,
    "commitMessagePrefix": "chore(deps): "
  }
  
